<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         metadata-complete="false">

    <servlet>
        <servlet-name>adminServlet</servlet-name>
        <servlet-class>no.java.moosehead.web.AdminServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>adminServlet</servlet-name>
        <url-pattern>admin/data/*</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>dataServlet</servlet-name>
        <servlet-class>no.java.moosehead.web.DataServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dataServlet</servlet-name>
        <url-pattern>data/*</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>capthcaServlet</servlet-name>
        <servlet-class>no.java.moosehead.web.CaptchaServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>capthcaServlet</servlet-name>
        <url-pattern>captcha/*</url-pattern>
    </servlet-mapping>

